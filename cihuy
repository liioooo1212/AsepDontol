-- Asep Kontol GUI Script (Executor Ready)
-- By ChatGPT

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")

-- Hapus GUI lama kalau ada
if PlayerGui:FindFirstChild("AsepKontolGUI") then
    PlayerGui:FindFirstChild("AsepKontolGUI"):Destroy()
end

-- Data koordinat
local teleportPoints = {
    Vector3.new(-1586, 118, 498),
    Vector3.new(-1174, 210, -21),
    Vector3.new(-805, 322, 617),
    Vector3.new(475, 190, 786),
    Vector3.new(492, 531, 619),
    Vector3.new(166, 1012, -125),
    Vector3.new(852, 1181, -481),
    Vector3.new(572, 1669, -326),
}

-- GUI utama
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "AsepKontolGUI"
screenGui.IgnoreGuiInset = false
screenGui.ResetOnSpawn = false
screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
screenGui.Parent = PlayerGui

local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 250, 0, 150)
mainFrame.Position = UDim2.new(0.5, -125, 0.3, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
mainFrame.BorderColor3 = Color3.fromRGB(0,0,0)
mainFrame.BorderSizePixel = 3
mainFrame.Active = true
mainFrame.Draggable = false -- kita bikin manual
mainFrame.Parent = screenGui

-- Header
local header = Instance.new("Frame")
header.Size = UDim2.new(1, 0, 0, 30)
header.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
header.BorderSizePixel = 0
header.Parent = mainFrame

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, -40, 1, 0)
title.Position = UDim2.new(0, 5, 0, 0)
title.BackgroundTransparency = 1
title.Text = "Asep Kontol"
title.Font = Enum.Font.SourceSansBold
title.TextSize = 18
title.TextColor3 = Color3.fromRGB(255,255,255)
title.TextXAlignment = Enum.TextXAlignment.Left
title.Parent = header

local minimizeButton = Instance.new("TextButton")
minimizeButton.Size = UDim2.new(0, 30, 1, 0)
minimizeButton.Position = UDim2.new(1, -35, 0, 0)
minimizeButton.Text = "-"
minimizeButton.Font = Enum.Font.SourceSansBold
minimizeButton.TextSize = 20
minimizeButton.BackgroundColor3 = Color3.fromRGB(200,50,50)
minimizeButton.TextColor3 = Color3.fromRGB(255,255,255)
minimizeButton.Parent = header

-- Toggle button
local toggleBtn = Instance.new("TextButton")
toggleBtn.Size = UDim2.new(0.6, 0, 0.3, 0)
toggleBtn.Position = UDim2.new(0.2, 0, 0.4, 0)
toggleBtn.Text = "Auto: OFF"
toggleBtn.Font = Enum.Font.SourceSansBold
toggleBtn.TextSize = 20
toggleBtn.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
toggleBtn.TextColor3 = Color3.fromRGB(255,255,255)
toggleBtn.Parent = mainFrame

-- Status Label
local statusLabel = Instance.new("TextLabel")
statusLabel.Size = UDim2.new(1, -10, 0, 20)
statusLabel.Position = UDim2.new(0, 5, 1, -25)
statusLabel.BackgroundTransparency = 1
statusLabel.Font = Enum.Font.SourceSans
statusLabel.TextSize = 14
statusLabel.TextColor3 = Color3.fromRGB(200,200,200)
statusLabel.Text = "Status: Standby"
statusLabel.Parent = mainFrame

-- Dragging (PC & Mobile)
local dragging, dragStart, startPos
header.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or 
       input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = mainFrame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

header.InputChanged:Connect(function(input)
    if (input.UserInputType == Enum.UserInputType.MouseMovement or 
        input.UserInputType == Enum.UserInputType.Touch) and dragging then
        local delta = input.Position - dragStart
        mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, 
                                       startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

-- Minimize
local minimized = false
minimizeButton.MouseButton1Click:Connect(function()
    minimized = not minimized
    if minimized then
        for _, obj in pairs(mainFrame:GetChildren()) do
            if obj ~= header then obj.Visible = false end
        end
        mainFrame.Size = UDim2.new(0, 180, 0, 30)
    else
        for _, obj in pairs(mainFrame:GetChildren()) do
            obj.Visible = true
        end
        header.Visible = true
        mainFrame.Size = UDim2.new(0, 250, 0, 150)
    end
end)

-- Auto Teleport
local auto = false
local running = false

local function teleportLoop()
    if running then return end
    running = true
    while auto do
        for _, point in ipairs(teleportPoints) do
            if not auto then break end
            local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                hrp.CFrame = CFrame.new(point)
                statusLabel.Text = ("Teleported to (%.0f, %.0f, %.0f)"):format(point.X, point.Y, point.Z)
            end
            wait(3)
        end
    end
    running = false
end

toggleBtn.MouseButton1Click:Connect(function()
    auto = not auto
    if auto then
        toggleBtn.Text = "Auto: ON"
        toggleBtn.BackgroundColor3 = Color3.fromRGB(50,200,50)
        statusLabel.Text = "Auto teleport aktif"
        teleportLoop()
    else
        toggleBtn.Text = "Auto: OFF"
        toggleBtn.BackgroundColor3 = Color3.fromRGB(70,70,70)
        statusLabel.Text = "Status: Standby"
    end
end)

-- Saat respawn, kalau auto ON, teleport ke point pertama
player.CharacterAdded:Connect(function(char)
    wait(1)
    if auto then
        local hrp = char:WaitForChild("HumanoidRootPart",5)
        if hrp then hrp.CFrame = CFrame.new(teleportPoints[1]) end
    end
end)
